{
  "name": "insighttrader",
  "version": "1.0.0",
  "description": "Sistema escalável para análises técnicas de ativos financeiros",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "lint": "eslint src/",
    "build": "docker build -t insighttrader .",
    "docker:run": "docker run -p 3000:3000 insighttrader",
    "seed:assets": "node src/scripts/seedAssets.js",
    "seed:techniques": "node src/scripts/seedTechniques.js",
    "seed:all": "npm run seed:techniques && npm run seed:assets",
    "seed:clear": "node src/scripts/seedTechniques.js --clear && node src/scripts/seedAssets.js --clear",
    "extract:logos": "node src/scripts/extractTradingViewLogos.js"
  },
  "keywords": ["finance", "trading", "technical-analysis", "nodejs", "api"],
  "author": "InsightTrader Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "axios": "^1.5.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.2",
    "joi": "^17.9.2",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.4",
    "supertest": "^6.3.3",
    "eslint": "^8.47.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}